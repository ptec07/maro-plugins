{
  "name": "google-calendar-mcp",
  "version": "1.0.0",
  "description": "MCP server for natural language Google Calendar control",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "mcp",
    "google-calendar",
    "claude",
    "ai",
    "assistant"
