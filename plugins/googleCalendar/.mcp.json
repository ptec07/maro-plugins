{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    },
    "google-calendar": {
      "command": "C:\Program Files\nodejs\node.exe",
      "args": ["dist/index.js"],
      "cwd": "${pluginRoot}"
    }
  },
  "staggeredStartup": {
    "enabled": true,
    "delayMs": 500,
    "connectionTimeout": 15000
  }
}
